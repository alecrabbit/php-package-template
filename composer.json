{
  "name": "alecrabbit/php-package-template",
  "description": "PHP package template",
  "type": "library",
  "repositories": [
    {
      "type": "vcs",
      "url": "/var/packages/helpers"
    }
  ],
  "require": {
    "php": ">=7.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.1",
    "nunomaduro/collision": "^2.1",
    "symfony/var-dumper": "^4.2"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "AlecRabbit\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AlecRabbit\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "beta",
  "prefer-stable": true
}
